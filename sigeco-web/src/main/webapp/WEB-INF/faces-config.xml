<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
	<converter>
		<converter-id>matrixConverter</converter-id>
		<converter-class>
			sigeco.web.jsf.converters.MatrixConverter
		</converter-class>
	</converter>
	<converter>
		<converter-for-class>sigeco.model.Matrix</converter-for-class>
		<converter-class>
			sigeco.web.jsf.converters.MatrixConverter
		</converter-class>
	</converter>
	<converter>
		<converter-for-class>sigeco.model.User</converter-for-class>
		<converter-class>
			sigeco.web.jsf.converters.UserConverter
		</converter-class>
	</converter>
	<converter>
		<converter-id>userConverter</converter-id>
		<converter-class>
			sigeco.web.jsf.converters.UserConverter
		</converter-class>
	</converter>
	<converter>
		<converter-for-class>
			sigeco.model.Knowledge
		</converter-for-class>
		<converter-class>
			sigeco.web.jsf.converters.KnowledgeConverter
		</converter-class>
	</converter>
	<converter>
		<converter-for-class>sigeco.model.Ability</converter-for-class>
		<converter-class>
			sigeco.web.jsf.converters.AbilityConverter
		</converter-class>
	</converter>
	<converter>
		<converter-for-class>sigeco.model.Grade</converter-for-class>
		<converter-class>
			sigeco.web.jsf.converters.GradeConverter
		</converter-class>
	</converter>
	<converter>
		<converter-for-class>
			sigeco.model.search.Operator
		</converter-for-class>
		<converter-class>
			sigeco.web.jsf.converters.OperatorConverter
		</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>userManagerBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.UserManagerBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userGroupManagerBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.UserGroupManagerBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>userGroupManager</property-name>
			<value>#{securedUserGroupManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>permissionBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.PermissionBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>errorBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.ErrorBean
		</managed-bean-class>
		<managed-bean-scope>none</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>userListBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.UserList
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
		<managed-property>
			<property-name>permissionBean</property-name>
			<value>#{permissionBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>userEditionBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.UserEditionBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.ViewBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userGroupEditionBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.UserGroupEditionBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
		<managed-property>
			<property-name>userGroupManager</property-name>
			<value>#{securedUserGroupManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>matrixManagerBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.MatrixManagerBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>matrixManager</property-name>
			<value>#{securedMatrixManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>newSearchBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.NewSearchBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>matrixManager</property-name>
			<value>#{matrixManager}</value>
		</managed-property>
		<managed-property>
			<property-name>searchManager</property-name>
			<value>#{searchManager}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>listSearchBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.ListSearchBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>searchManager</property-name>
			<value>#{searchManager}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>resultBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.ResultBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>searchManager</property-name>
			<value>#{searchManager}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>matrixEditionBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.MatrixEditionBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>matrixFeederBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.MatrixFeederBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>matrixManager</property-name>
			<value>#{matrixManager}</value>
		</managed-property>
		<managed-property>
			<property-name>cellValueManager</property-name>
			<value>#{cellValueManager}</value>
		</managed-property>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.LoginBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewMatrixFeedBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.ViewMatrixFeedBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
		<managed-property>
			<property-name>matrixManager</property-name>
			<value>#{securedMatrixManager}</value>
		</managed-property>
		<managed-property>
			<property-name>cellValueManager</property-name>
			<value>#{cellValueManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>renderableMatrix</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.RenderableMatrix
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>userManager</property-name>
			<value>#{securedUserManager}</value>
		</managed-property>
		<managed-property>
			<property-name>matrixManager</property-name>
			<value>#{securedMatrixManager}</value>
		</managed-property>
		<managed-property>
			<property-name>cellValueManager</property-name>
			<value>#{cellValueManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cellValueMapBean</managed-bean-name>
		<managed-bean-class>
			sigeco.web.jsf.beans.CellValueMapBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>cellValueManager</property-name>
			<value>#{cellValueManager}</value>
		</managed-property>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>matrix list</from-outcome>
			<to-view-id>/matrices/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>user list</from-outcome>
			<to-view-id>/users/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>user group list</from-outcome>
			<to-view-id>/users/groupList.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>feed list</from-outcome>
			<to-view-id>/feeds/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view feed list</from-outcome>
			<to-view-id>/views/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view search home</from-outcome>
			<to-view-id>/search/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login successful</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/matrices/list.jsp</from-view-id>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/matrices/matrixGeneralData.jsp</from-view-id>
		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>/matrices/matrixKnowledges.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/matrices/matrixKnowledges.jsp</from-view-id>
		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>/matrices/matrixAbilities.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>restart</from-outcome>
			<to-view-id>/matrices/matrixGeneralData.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>previous</from-outcome>
			<to-view-id>/matrices/matrixGeneralData.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/matrices/matrixAbilities.jsp</from-view-id>
		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>/matrices/matrixCells.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>restart</from-outcome>
			<to-view-id>/matrices/matrixGeneralData.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>previous</from-outcome>
			<to-view-id>/matrices/matrixKnowledges.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/matrices/matrixCells.jsp</from-view-id>
		<navigation-case>
			<from-outcome>restart</from-outcome>
			<to-view-id>/matrices/matrixGeneralData.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/feeds/list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>select</from-outcome>
			<to-view-id>/feeds/dragDropAbilities.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/feeds/dragDropAbilities.jsp</from-view-id>
		<navigation-case>
			<from-outcome>finish</from-outcome>
			<to-view-id>/feeds/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/users/list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editUser</from-outcome>
			<to-view-id>/users/user.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editProfile</from-outcome>
			<to-view-id>/users/profile.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>showProfile</from-outcome>
			<to-view-id>/users/showProfile.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/users/user.jsp</from-view-id>
		<navigation-case>
			<from-outcome>restart</from-outcome>
			<to-view-id>/users/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/users/profile.jsp</from-view-id>
		<navigation-case>
			<from-outcome>restart</from-outcome>
			<to-view-id>/users/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/users/showProfile.jsp</from-view-id>
		<navigation-case>
			<from-outcome>restart</from-outcome>
			<to-view-id>/users/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/users/groupList.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editGroup</from-outcome>
			<to-view-id>/users/groupEdition.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/users/groupEdition.jsp</from-view-id>
		<navigation-case>
			<from-outcome>backToList</from-outcome>
			<to-view-id>/users/groupList.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/search/home.jsp</from-view-id>
		<navigation-case>
			<from-outcome>new search</from-outcome>
			<to-view-id>/search/new.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>list searches</from-outcome>
			<to-view-id>/search/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/search/list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>new search</from-outcome>
			<to-view-id>/search/new.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>list searches</from-outcome>
			<to-view-id>/search/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>show result</from-outcome>
			<to-view-id>/search/search.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/search/view.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>	
	<navigation-rule>
		<from-view-id>/search/view.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/search/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>show result</from-outcome>
			<to-view-id>/search/search.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/views/list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>select</from-outcome>
			<to-view-id>/views/matrix.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>selectAll</from-outcome>
			<to-view-id>/views/userMatrices.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/views/userMatrices.jsp</from-view-id>
		<navigation-case>
			<from-outcome>voltar</from-outcome>
			<to-view-id>/views/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/views/matrix.jsp</from-view-id>
		<navigation-case>
			<from-outcome>voltar</from-outcome>
			<to-view-id>/views/list.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>newSearch</from-outcome>
			<to-view-id>/search/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/search/search.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back to list</from-outcome>
			<to-view-id>/search/list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back to form</from-outcome>
			<to-view-id>/search/new.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back to view</from-outcome>
			<to-view-id>/search/view.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/search/new.jsp</from-view-id>
		<navigation-case>
			<from-outcome>cancel search</from-outcome>
			<to-view-id>/search/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>search saved</from-outcome>
			<to-view-id>/search/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>show result</from-outcome>
			<to-view-id>/search/search.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<message-bundle>validation</message-bundle>
		<locale-config>
			<default-locale>pt-BR</default-locale>
		</locale-config>
		<variable-resolver>
			org.springframework.web.jsf.DelegatingVariableResolver
		</variable-resolver>
	</application>
</faces-config>
